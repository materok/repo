\begin{frame}{CMS-SUS-15-002}
	\visible{
		\begin{textblock}{8}(0,5)
			Event selection: 
			\begin{itemize}
			 \item event: \MET{} $>$ 30~GeV
			 \item jet: 
			\begin{itemize}
				\item \pt{} $>$ 30~GeV; 20~GeV for b-jet
				\item $\left| \eta \right|<$ 2.5
				\item loose jet requirements on composition
				\item $\Delta\phi_{min}$(\MET, leading 4 jets)$>$0.3)
			\end{itemize}
			 \item imbalance $\frac{ \left| \vec{E}^{miss}_T -\vec{H}^{miss}_T \right| }{E^{miss}_T} <$ 0.5
			\end{itemize}
			
		\end{textblock}
		\begin{textblock}{8.5}(8,3)
		 \onslide<1->{signal regions:
			\begin{itemize}
			 \item 5 bins in \HT{} ($>$ 200~GeV)
			 \item 11 bins in N$_J$ ($>$ 2) and N$_b$ ($\geq$ 0)
			 \item topological regions: for each of the 55 bins, fill 22 \MTT{} bins
			\end{itemize}
		}
		\only<1>{\includegraphics[width=.9\textwidth]{src/CMS-PAS-SUS-15-003_Figure_007-a}
		}
		%\onslide<4>{\includegraphics[width=.9\textwidth]{src/CMS-PAS-SUS-15-003_Figure_007-b}
		%}
		\end{textblock}

	}
\end{frame}



