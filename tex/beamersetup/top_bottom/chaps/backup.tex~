\begin{frame}{CMS-SUS-15-003}
	\visible{
		\begin{textblock}{16}(0,3)
			\centering
			data used: $\mathcal{L}_{int}$=2.2~fb$^{-1}$, $\sqrt{s}$=13~TeV\\
			3 triggers: \HT{} and \MET{} dependent thresholds
			
		\end{textblock}
		\begin{textblock}{10}(0,6)
			backgrounds:
			\begin{itemize}
			 \item<1-> 'lost lepton'
			 \begin{itemize}
			  \item<1|only@1> W+jets
			  \item<1|only@1> t$\bar{\text{t}}$+jets
			  \item<1|only@1> lepton out of acceptance, not reconstructed, not identifier or not isolated
			  \item<1|only@1> estimated with lepton control sample by inverting lepton veto in each topological region
			 \end{itemize}
			  \item<2-> 'irreducible'
			\begin{itemize}
			 \item<2|only@2> Z+jets
			  \item<2|only@2> most similar topology
			 \item<2|only@2> decreases in importance with increasing N$_b$
			 \item<2|only@2> estimated with $\gamma$+jets control sample in each topological region
			\end{itemize}
			\item<3-> 'instrumental background'
			\begin{itemize}
			 \item<3|only@3> QCD multi-jet with no real \MET{}
			 \item<3|only@3> enter by significant jet mismeasurement or sources of anomalous noise.
			 \item<3|only@3> suppressed by event selection
			 \item<3|only@3> estimated by inverting $\Delta\phi_{min}$ requirement in each topological region
			\end{itemize}
			 %\item<3-> mostly data-driven estimates
			\end{itemize}
		\end{textblock}
		\begin{textblock}{6.5}(9.75,5)
		\only<3>{\includegraphics[width=.9\textwidth]{src/gluonfd.png} \\ \tiny adapted from \\\url{http://oer.physics.manchester.ac.uk/NP/Notes/Notes/Notesse54.xht}}
		\only<2>{\includegraphics[width=\textwidth]{src/figures_p28_fig10_1}\\ \tiny adapted from \url{https://inspirehep.net/record/1082043/plots}}
		\only<1>{\includegraphics[width=\textwidth]{src/ttLJT} \\ \tiny adapted from \url{https://inspirehep.net/record/871832/plots}}
		\end{textblock}

	}
\end{frame}

