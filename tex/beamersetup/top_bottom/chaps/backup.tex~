\end{frame}

\begin{frame}{CMS-SUS-15-011}
	\visible{
		\begin{textblock}{16}(0,2.5)
			\centering
			\color{black} data used: $\mathcal{L}_{int}$=2.2~fb$^{-1}$, $\sqrt{s}$=13~TeV\\
			2 triggers with different \pt{} threshold\\
			lepton \pt{} $>$ 20~GeV
			
		\end{textblock}
		\begin{textblock}{9.75}(0,6)
			\visible<1->{backgrounds:}
\begin{itemize}
 \item<1-> Flavour-symmetric (FS) \only<1>{produce opposite flavour pairs ($e^\pm \mu^\mp$) as often as same flavour pair ($e^\pm e^\mp$ or $ \mu^\pm \mu^\mp$)}
\begin{itemize}
	\item<2|only@2> \color{violet}t$\bar{\text{t}}$\color{black}
	\item<2|only@2> \color{blue}WW\color{black}
	\item<2|only@2> Z/$\gamma$* ($\rightarrow \tau\tau$)
	\item<2|only@2> \color{blue}tW\color{black}
	\item<2|only@2> \color{blue}single top production\color{black}
	\item<2|only@2> lepton from hadron decay
	\item<2|only@2> hadron misidentified as lepton
	\item<3|only@3> symmetry might be distorted (e.g. trigger, reco, ID)
	\item<3|only@3> estimate opposite flavor contribution from same flavor contribution
	\item<3|only@3> multiplicative factor R$_{SF/OF}$
	\item<3|only@3> direct measurement in control region (enriched)
	\item<3|only@3> measurement of lepton efficiencies
\end{itemize}
\item<4-> Drell-Yan-like (DY) \only<4>{flavour-correlated}
\begin{itemize}
	\item<4|only@4> \color{blue}DY+jets \color{black}(\MET{} from mismeasurement of jets)
	\item<4|only@4> \color{blue}WZ\color{black}
	\item<4|only@4> \color{blue}ZZ\color{black}
	\item<4|only@4> t$\bar{\text{t}}$Z
	\item<5|only@5> \MET{} for DY derived from template
	\item<5|only@5> assumption: \MET{} from detector resolution (jets recoil from Z)
	\item<5|only@5> control sample of $\gamma$ +jets
	\item<5|only@5> \MET{} for rest from MC
\end{itemize}

\end{itemize}

		\end{textblock}
		\begin{textblock}{6.5}(9.75,5)
			\only<1-3>{\includegraphics[width=\textwidth]{src/qq.png}\\ \tiny adapted from \url{https://hep.pa.msu.edu/wiki/Computing/MadEvent}}
			\visible<4->{\includegraphics[width=\textwidth]{src/drell-yan-feyn.pdf}\\ \tiny adapted from \url{http://inspirehep.net/record/943750/plots}}
		\end{textblock}

		\begin{textblock}{6.5}(9.75,14)
			\footnotesize
			\only<2,4>{\color{blue} MadGraph5\_aMC@NLO\color{black}\\
			\color{violet} POWHEG \color{black}}
		\end{textblock}
	}
\end{frame}
\begin{frame}{On-Z search}
	\visible{
		\begin{textblock}{8}(0,4)
			3 signal regions with subregions:
\begin{itemize}
\item<1-> SRA
\begin{itemize}
\item<2|only@2> low jet-multiplicity (2 or 3)
\item<2|only@2> number of b-tagged jets N$_b$
\item<2|only@2> \HT{} ($>$ 400~GeV)
\item<2|only@2> \MET{} ($>$ 100~GeV)
\end{itemize}
\item<1-> SRB
\begin{itemize}
\item<3|only@3> high jet-multiplicity ($\geq$ 4)
\item<3|only@3> number of b-tagged jets N$_b$
\item<3|only@3> \HT
\item<3|only@3> \MET{} ($>$100~GeV)
\end{itemize}
\item<1-> 'ATLAS-like' region
\begin{itemize}
\item<4|only@4> requirement on \HT{}+\pt$^{l_1}$+\pt$^{l_2}$
\item<4|only@4> \MET{} ($>$225~GeV)
\item<4|only@4> $\Delta\phi_{\text{\MET},j_1,j_2}$ ($>$ 0.4)
\item<4|only@4> $\Delta$R between leptons $>$ 0.1 
\end{itemize}
\end{itemize}

		\end{textblock}
		\begin{textblock}{8}(8,4)
		 \centering
		 ATLAS 3$\sigma$ excess in 8~TeV data
		 \only<1,2>{\includegraphics[width=\textwidth]{src/fig_12a.pdf}}
		 \visible<3->{\includegraphics[width=\textwidth]{src/fig_12b.pdf}}
		\end{textblock}

	}
\end{frame}


\begin{frame}{Off-Z search}
	\visible{
		\begin{textblock}{8}(0,4)
			2 regions with subregions:
			for both regions: \MET{} $>$ 150(100)~GeV for N$_{jets} >=$2(3)
\begin{itemize}
\item central{: Both leptons in $\left|\eta\right|<$1.4}
\item forward{: At least one lepton in $\left|\eta\right|>$1.4}
\begin{itemize}
\item number of b-tagged jets N$_b$
\begin{itemize}
\item<1|only@1> =0
\item<1|only@1> $\geq$1
\item<1|only@1> $\geq$0 (inclusive, for comparability with 8~TeV data)
\end{itemize}
\item m$_{ll}$
\begin{itemize}
\item<2> 20-70~GeV
\item<2> 70-81~GeV
\item<2> 81-101~GeV
\item<2> 101-120~GeV
\item<2> $>$ 120~GeV
\end{itemize}

\end{itemize}

\end{itemize}

		\end{textblock}
		\begin{textblock}{8}(8,4)
		 \centering
		 CMS 2.6$\sigma$ excess in 8~TeV data
		 \only<1>{\includegraphics[width=\textwidth]{src/Fixed_Edge_sbottom_neutralino2_Exclusion_witXsecLimit.pdf}}
		 \only<2>{\includegraphics[width=\textwidth]{src/Fixed_Neutralino_sbottom_neutralino2_Exclusion_witXsecLimit.pdf}}
		\end{textblock}

	}
\end{frame}


