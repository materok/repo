\subsection{sps}
\begin{frame}{SPS}
		\visible{
		\begin{textblock}{7.5}(8.25,5.5) %{width}(x,y)
	%		\includegraphics[width=\textwidth]{src/ps_schema}
		\end{textblock}
		\begin{textblock}{8}(7.75,5.25) %{width}(x,y)
			\includegraphics[width=\textwidth]{src/basis}
		\end{textblock}
		\begin{textblock}{8}(.25,6.5) %{width}(x,y)
			8  400.8 MHz RF cavities per beam\\
			1232 dipolemagnets\\
			392  quadropole magnets\\
			688  sextupole magnets\\
			168  octupole magnets\\
			10$^{11}$ protons per bunch
		\end{textblock}
		\begin{textblock}{16}(0.05,3) %{width}(x,y)
			\textbf{L}arge \textbf{H}adron \textbf{C}ollider (LHC)\\
			26659m circumference synchrotron used accelerate 13 SPS fills (2808 bunches) to 6.5 TeV
		\end{textblock}
		\begin{textblock}{7.5}(.25,3.5) %{width}(x,y)
			%hydrogen bottle with duoplasmatron
		\end{textblock}
		\begin{textblock}{13}(15.75,3.75) %{width}(x,y)
			\begin{turn}{270}
